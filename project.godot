config_version=3

[application]

config/name="CoS"
run/main_scene="res://cutscenes/CutScene1.tscn"
boot_splash/image="res://splash.png"
config/icon="res://icon.png"

[autoload]

global="*res://scripts/global.gd"
util="*res://scripts/util.gd"
colors="*res://scripts/colors.gd"
AutoResize="*res://scripts/AutoResize.gd"

[display]

window/size/width=800
window/size/resizable=false
window/handheld/emulate_mouse_from_touch=false

